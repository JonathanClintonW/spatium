{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "singularName": "member",
    "pluralName": "members",
    "displayName": "Member",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "dob": {
      "type": "date",
      "required": true
    },
    "point": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "password": {
      "type": "password",
      "required": true
    },
    "confirmed": {
      "type": "boolean",
      "default": true
    },
    "provider": {
      "type": "string",
      "default": "local"
    },
    "uuid": {
      "type": "uid",
      "required": false
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address"
    },
    "carts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart.cart"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address",
      "mappedBy": "member"
    }
  }
}
